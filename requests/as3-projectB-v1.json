{
  "class": "AS3",
  "action": "deploy",
  "persist": true,
  "declaration": {
    "class": "ADC",
    "schemaVersion": "3.0.0",
    "id": "example-declaration-01",
    "label": "Sample 1",
    "remark": "Simple HTTP application with round robin pool",
    "Project_B": {
      "class": "Tenant",
      "Nginx": {
        "class": "Application",
        "nginx_http": {
          "class": "Service_HTTP",
          "virtualAddresses": [
            "10.1.20.200"
          ],
          "pool": "nginx_pool"
        },
        "nginx_pool": {
          "class": "Pool",
          "monitors": [
            "http"
          ],
          "members": [
            {
              "servicePort": 8080,
              "serverAddresses": [
                "10.1.10.5"
              ]
            }
          ]
        }
      }
    }
  }
}